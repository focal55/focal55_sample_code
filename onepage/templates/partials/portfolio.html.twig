            <div class="col-md-12 animated" data-animation="fadeInUp" data-animation-delay="100">
                <div id="options">
                    <ul id="filters" class="option-set clearfix" data-option-key="filter">
                        <li><a href="#filter" data-option-value="*" class="selected"><i class="fa fa-th"></i></a></li>
                        <li><a href="#filter" data-option-value=".graphics"><i class="fa fa-heart"></i> </a></li>
                        <li><a href="#filter" data-option-value=".photos"><i class="fa fa-picture-o"></i></a></li>
                        <li><a href="#filter" data-option-value=".videos"><i class="fa fa-video-camera"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
